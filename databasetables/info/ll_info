create table ll_info
(
    ll_picture_ID SERIAL PRIMARY KEY,
    ll_dateTaken TIMESTAMP NOT NULL,
    ll_image_age INT,
    ll_erythema_score INT,
    ll_edema_score INT,
    ll_excloration_score INT,
    ll_lichenification_score INT,
    ll_area_score INT,
    ll_total_score INT,
    ll_complete bit,
    ll_comments VARCHAR(250)
);
